{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
	  "source": "~/.config/fastfetch/nixos_custom_cat.png",
	  "height":14

  },
  "display": {
	  "separator": "  ",
    "color": {
      "keys": "magenta",
      "separator": "90"
    },
  },
  "modules": [
    {
      "type": "custom",
      "format": "{#34}{#}{#bright_inverse_34} icdt {#}{#34}{#}{#36}{#}{#bright_inverse_36} nix-top {#}{#36}{#}"
    },
    "break",
    {
	    "type": "custom",
	    "format": "{#90}==/=/=========================={#}"
    }, 
    "break",
    {
      "type": "os",
      "key": "{#blue} {#} {#bright_cyan}OSYS{#}",
      "format": "{name} {version-id}"
    },
    {
      "type": "host",
      "key": "{#blue}󰨇 {#} {#bright_cyan}HOST{#}"
    },
    {
      "type": "uptime",
      "key": "{#blue} {#} {#bright_cyan}UPTM{#}",
      "format": "{hours} hours {minutes} minutes"
    },
    {
      "type": "wm",
      "key": "{#blue} {#} {#bright_cyan}WMAN{#}",
      "format": "{pretty-name} {version}"
    },
    {
      "type": "terminal",
      "key": "{#blue} {#} {#bright_cyan}TERM{#}"
    },

    "break",
    {
	    "type": "custom",
	    "format": "{#90}==/=/=========================={#}"
    }, 
    "break",

    {
      "type": "custom",
      "format": "{#30}█{#} {#31}█{#} {#32}█{#} {#33}█{#} {#34}█{#} {#35}█{#} {#36}█{#} {#37}█{#}"
    }
  ]
}
